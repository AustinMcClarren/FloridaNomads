<!DOCTYPE html>
<html>
<head>
	<title>Traveller Profile</title>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.7.7/handlebars.min.js"></script>
</head>
<body>
	<div id="profile-template">
		<h1>{{name}}</h1>
		<p>{{bio}}</p>
		<p>Current Points: {{points}}</p>
		<p>email: {{email}}</p>
		<p>Interests:</p>
		<ul>
			{{#each Destination}}
			<li>{{this}}</li>
			{{/each}}
		</ul>
		<p>Recent Trips:</p>
		<ul>
			{{#each trips}}
			<li>{{Destination}} - {{city}}</li>
			{{/each}}
		</ul>
	</div>


<script src="./js/profile.js"></script>
